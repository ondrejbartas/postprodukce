<h2>Pracovní místa</h2>

<div class="menu_small">
  <%= link_to 'vytvořit nové místo', new_place_path %>
</div>

<table>
  <tr>
    <th>název</th>
    <th>poznámka</th>
  </tr>

<% for place in @places %>
  <tr>
    <td><%= link_to place.name, place %></td>
    <td><%=h place.note %></td>
    <td></td>
  </tr>
<% end %>
</table>

<br />

